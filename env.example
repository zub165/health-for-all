# Environment Configuration for Health For All Fair
# Copy this file to .env and update the values

# Django Backend URL (for production deployment)
# Replace with your actual Django backend URL
REACT_APP_BACKEND_URL=https://your-django-backend.herokuapp.com/api

# Example backend URLs:
# Heroku: https://your-app-name.herokuapp.com/api
# Railway: https://your-app-name.railway.app/api
# DigitalOcean: https://your-app-name.ondigitalocean.app/api
# AWS: https://your-app-name.elasticbeanstalk.com/api

# Development (local Django backend)
# REACT_APP_BACKEND_URL=http://localhost:3015/api

# Demo mode (no backend)
# REACT_APP_BACKEND_URL=
